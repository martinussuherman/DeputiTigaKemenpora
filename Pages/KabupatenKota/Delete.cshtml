@page "{id:int?}"
@model DeleteModel

@{
   Layout = "_CommonLayout";
}

<div class="card-body">
   <div class="form-group">
      <label class="control-label">Kode</label>
      <input asp-for="KabupatenKota.Id" class="form-control" disabled />
   </div>

   <div class="form-group">
      <label class="control-label">Nama</label>
      <input asp-for="KabupatenKota.Nama" class="form-control" disabled />
   </div>

   <div class="form-group">
      <label class="control-label">Provinsi</label>
      <input asp-for="KabupatenKota.NamaProvinsi" class="form-control" disabled />
   </div>

   <form method="post">
      <input type="hidden" asp-for="KabupatenKota.Id" />
      <input type="submit" value="Hapus" class="btn btn-danger" />
      <a asp-page="./Index" class="btn btn-warning">Kembali</a>
   </form>
</div>
